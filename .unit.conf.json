{
    "listeners": {
        "*:8080": {
            "pass": "routes"
        }
    },
    "routes": [
        {
            "action": {
                "share": "/www/app/$uri",
                "fallback": {
                    "pass": "applications/app"
                }
            }
        }
    ],
    "applications": {
        "app": {
            "type": "php",
            "root": "/www/app/",
            "script": "index.php"
        }
    }
}